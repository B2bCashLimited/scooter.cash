{
  "name": "b2b-promo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:test": "node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng build --aot --source-map=false",
    "build:prod": "node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng build --prod --source-map=false",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:ssr-test": "npm run build:client-and-server-bundles-test && npm run webpack:server",
    "build:ssr-live": "npm run build:client-and-server-bundles-prod && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles-test": "node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng build --configuration=development && ng run b2b-promo:server",
    "build:client-and-server-bundles-prod": "node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng build --prod --configuration=production && ng run b2b-promo:server",
    "webpack:server": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js --config webpack.server.config.js --progress --colors --mode=development"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.5",
    "@angular/animations": "~7.1.0",
    "@angular/cdk": "~7.1.1",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/material": "~7.1.1",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/platform-server": "^8.0.0",
    "@angular/router": "~7.1.0",
    "@ng-select/ng-select": "^2.19.0",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
    "@ngx-translate/core": "~11.0.1",
    "@ngx-translate/http-loader": "~4.0.0",
    "@types/express": "^4.17.0",
    "@types/lodash": "^4.14.110",
    "angular-gtag": "~1.0.3",
    "bootstrap": "~4.1.3",
    "core-js": "^2.5.4",
    "font-awesome": "4.7.0",
    "hammerjs": "~2.0.8",
    "linkifyjs": "^2.1.8",
    "lodash": "^4.17.10",
    "moment": "^2.24.0",
    "ng-pick-datetime": "^7.0.0",
    "ng2-file-upload": "^1.3.0",
    "ngx-gallery": "^5.9.1",
    "ngx-infinite-scroll": "^7.0.0",
    "ngx-mask": "^7.9.2",
    "ngx-pagination": "^3.2.1",
    "ngx-swiper-wrapper": "^7.2.1",
    "ngx-youtube-player": "^6.0.0",
    "rxjs": "~6.3.3",
    "socket.io-client": "^2.2.0",
    "ts-loader": "^6.0.2",
    "tslib": "^1.9.3",
    "zone.js": "~0.8.26"
  },
